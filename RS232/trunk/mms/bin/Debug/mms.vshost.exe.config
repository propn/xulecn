<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="mms.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
      <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net-net-1.2"/>
    </sectionGroup>
  </configSections>
  <connectionStrings>
    <add name="mms.Properties.Settings.dbConnectionString" connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\db.mdb;Persist Security Info=True"
      providerName="System.Data.OleDb" />
  </connectionStrings>
  <applicationSettings>
    <mms.Properties.Settings>
      <setting name="rpcURL" serializeAs="String">
        <value>http://218.206.69.203:80/eepwww/xmlrpc </value>
      </setting>
      <setting name="Customer_id" serializeAs="String">
        <value>1</value>
      </setting>
      <setting name="Corp_Account" serializeAs="String">
        <value>2001299904</value>
      </setting>
      <setting name="Token" serializeAs="String">
        <value>1924af76-9d14-4fa9-9c9c-993341988eb0</value>
      </setting>
      <setting name="FTPRemoteIP" serializeAs="String">
        <value />
      </setting>
      <setting name="FTPRemotePort" serializeAs="String">
        <value>0</value>
      </setting>
      <setting name="FTPUser" serializeAs="String">
        <value />
      </setting>
      <setting name="FTPPassword" serializeAs="String">
        <value />
      </setting>
      <setting name="SMSAgreement_id" serializeAs="String">
        <value>4</value>
      </setting>
      <setting name="MMSAgreement_id" serializeAs="String">
        <value>1</value>
      </setting>
    </mms.Properties.Settings>

    <log4net>
      <root>
        <level value="ALL" />
        <appender-ref ref="LogFileAppender" />
      </root>

      <appender name="LogFileAppender" type="log4net.Appender.FileAppender" >
        <param name="File" value="log-file.txt" />
        <param name="AppendToFile" value="true" />
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="[%utcdate{yyyy-MM-dd HH:mm:ss.ff}] [%thread] [%-5level] [%logger] - %message%newline" />
        </layout>
      </appender>

      <logger name="log4net">
        <level value="INFO" />
        <appender-ref ref="LogFileAppender" />
      </logger>
      
    </log4net>

  </applicationSettings>
</configuration>