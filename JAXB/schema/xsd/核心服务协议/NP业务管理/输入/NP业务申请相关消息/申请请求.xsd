<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2005 rel. 3 U (http://www.altova.com) by  () -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:complexType name="IdentityInfo">
		<xs:annotation>
			<xs:documentation>证件信息</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IdentType">
				<xs:annotation>
					<xs:documentation>证件类型</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="IdentNum">
				<xs:annotation>
					<xs:documentation>证件号码</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PropertyInfo">
		<xs:annotation>
			<xs:documentation>扩展属性</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ItemID" type="xs:string">
				<xs:annotation>
					<xs:documentation>属性标识ID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ItemName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>属性名称</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ItemValue" type="xs:string">
				<xs:annotation>
					<xs:documentation>属性值</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ItemValueName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>值名称</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Party">
		<xs:annotation>
			<xs:documentation>联系人</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="UserName" type="xs:string">
				<xs:annotation>
					<xs:documentation>姓名</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PartyType" type="xs:string">
				<xs:annotation>
					<xs:documentation>参与人类型</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IdentityInfo" type="IdentityInfo">
				<xs:annotation>
					<xs:documentation>身份信息</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Telephone" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>联系电话</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NetWorkID">
		<xs:annotation>
			<xs:documentation>网络ID</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PortOutNetID" type="xs:string">
				<xs:annotation>
					<xs:documentation>网络ID(携出方)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PortInNetID" type="xs:string">
				<xs:annotation>
					<xs:documentation>网络ID(携入方)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HomeNetID" type="xs:string">
				<xs:annotation>
					<xs:documentation>网络ID(号码拥有网络)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NPObjectSet">
		<xs:annotation>
			<xs:documentation>NP对象</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CommandCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>消息命令码</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ServiceType" type="xs:string">
				<xs:annotation>
					<xs:documentation>业务类型</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NpCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>NP号码</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlowID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>NP流程ID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NetWorkID" type="NetWorkID">
				<xs:annotation>
					<xs:documentation>网络ID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Party" type="Party" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>联系人</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CardType" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>携入后用户类型</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReqTime" type="xs:string">
				<xs:annotation>
					<xs:documentation>申请时间</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SuperSet" type="PropertyInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>用户其它属性</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NPObjectEvent">
		<xs:annotation>
			<xs:documentation>网间号码携带通知</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MessageID" type="xs:string">
				<xs:annotation>
					<xs:documentation>消息ID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NPObjectSet" type="NPObjectSet" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>NP对象</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Remark" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>备注</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SvcCont">
		<xs:annotation>
			<xs:documentation>业务对象</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="NPObjectEvent" type="NPObjectEvent">
				<xs:annotation>
					<xs:documentation>网间号码携带通知</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
