							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="1" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ReqCode">
							<xs:annotation>
								<xs:documentation>请求/扩展代码</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="4" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ReqDesc" minOccurs="0">
							<xs:annotation>
								<xs:documentation>请求/扩展描述</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="0" />
